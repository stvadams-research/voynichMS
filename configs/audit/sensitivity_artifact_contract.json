{
  "version": "2026-02-10-c1.5",
  "description": "SK-C1.5 sensitivity artifact/report contract for CI and release gating.",
  "artifact_path": "status/audit/sensitivity_sweep.json",
  "report_path": "reports/audit/SENSITIVITY_RESULTS.md",
  "artifact_path_by_mode": {
    "ci": "status/audit/sensitivity_sweep.json",
    "release": "status/audit/sensitivity_sweep_release.json"
  },
  "report_path_by_mode": {
    "ci": "reports/audit/SENSITIVITY_RESULTS.md",
    "release": "reports/audit/SENSITIVITY_RESULTS_RELEASE.md"
  },
  "required_summary_fields": [
    "schema_version",
    "policy_version",
    "generated_utc",
    "generated_by",
    "dataset_id",
    "dataset_pages",
    "dataset_tokens",
    "dataset_policy_pass",
    "warning_policy_pass",
    "warning_density_per_scenario",
    "total_warning_count",
    "caveats",
    "quality_gate_passed",
    "robustness_conclusive",
    "execution_mode",
    "scenario_count_expected",
    "scenario_count_executed",
    "release_readiness_failures",
    "release_evidence_ready",
    "robustness_decision"
  ],
  "required_summary_bool_fields": [
    "dataset_policy_pass",
    "warning_policy_pass",
    "quality_gate_passed",
    "robustness_conclusive",
    "release_evidence_ready"
  ],
  "required_summary_list_fields": [
    "caveats",
    "release_readiness_failures"
  ],
  "report_field_contract": [
    {
      "label": "Dataset",
      "summary_key": "dataset_id",
      "format": "str"
    },
    {
      "label": "Dataset pages",
      "summary_key": "dataset_pages",
      "format": "int"
    },
    {
      "label": "Dataset tokens",
      "summary_key": "dataset_tokens",
      "format": "int"
    },
    {
      "label": "Execution mode",
      "summary_key": "execution_mode",
      "format": "str"
    },
    {
      "label": "Scenario execution",
      "format": "scenario_fraction"
    },
    {
      "label": "Release evidence ready (full sweep + conclusive robustness + quality gate)",
      "summary_key": "release_evidence_ready",
      "format": "bool"
    },
    {
      "label": "Robustness decision",
      "summary_key": "robustness_decision",
      "format": "str"
    },
    {
      "label": "Dataset policy pass",
      "summary_key": "dataset_policy_pass",
      "format": "bool"
    },
    {
      "label": "Warning policy pass",
      "summary_key": "warning_policy_pass",
      "format": "bool"
    },
    {
      "label": "Warning density per scenario",
      "summary_key": "warning_density_per_scenario",
      "format": "float2"
    },
    {
      "label": "Total warnings observed",
      "summary_key": "total_warning_count",
      "format": "int"
    }
  ],
  "release_mode_requirements": {
    "execution_mode": "release",
    "release_evidence_ready": true,
    "dataset_policy_pass": true,
    "warning_policy_pass": true,
    "quality_gate_passed": true,
    "robustness_conclusive": true,
    "scenario_counts_must_match": true,
    "release_readiness_failures_must_be_empty": true
  },
  "runtime_contract": {
    "preflight_path": "status/audit/sensitivity_release_preflight.json",
    "run_status_path": "status/audit/sensitivity_release_run_status.json",
    "max_preflight_age_seconds": 21600,
    "max_release_artifact_age_seconds": 259200,
    "max_run_heartbeat_age_seconds": 1800
  },
  "caveat_policy": {
    "forbid_none_phrase_when_warnings_present": true,
    "none_phrase_pattern": "Caveat: none",
    "require_nonempty_caveat_list_when_warnings_present": true
  }
}
